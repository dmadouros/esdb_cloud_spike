<h1 class="text-4xl text-gray-500 pb-8">Message</h1>
<table class="w-full">
  <thead>
  <tr class="border-t border-b border-slate-300 font-bold text-left">
    <th class="py-4 pr-4">Event ID</th>
    <th class="py-4 pr-4">Stream</th>
    <th class="py-4 pr-4">Type</th>
    <th class="py-4 pr-4">Timestamp</th>
  </tr>
  </thead>
  <tbody>
  <tr class="border-t border-b border-slate-300">
    <td class="py-4 pr-4"><%= @message.event_id %></td>
    <td class="py-4 pr-4"><%= @message.stream %></td>
    <td class="py-4 pr-4"><%= @message.event_type %></td>
    <td class="py-4 pr-4"><%= render partial: "shared/timestamp", locals: {timestamp: @message.timestamp} %></td>
  </tr>
  </tbody>
  <thead>
  <tr class="border-t border-slate-300 font-bold text-left">
    <th  class="py-4 pr-4" colspan="4">Data</th>
  </tr>
  </thead>
  <tbody>
  <tr class="border-b border-slate-300">
    <td  class="py-4 pr-4" colspan="4">
      <pre class="text-gray-600">
<%= JSON.pretty_generate(@message.data) %>
</pre>
    </td>
  </tr>
  </tbody>
</table>
